{
    "compilerOptions": {
        "target": "ES2019",
        "module": "Node16",
        "moduleResolution": "Node16",
        "resolveJsonModule": true,
        "declaration": true,
        "outDir": "dist",
        "rootDir": "src",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true
    },
    "include": [
        "src"
    ]
}